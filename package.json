{
  "name": "my-orders-tickets-widget",
  "version": "1.0.0",
  "main": "index.js",
  "author": "smarcet <smarcet@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rm -R dist & rm -R node_modules & yarn",
    "build-dev": "./node_modules/.bin/webpack --config webpack.dev.js",
    "build": "./node_modules/.bin/webpack --config webpack.prod.js",
    "serve": "webpack-dev-server --open --server-type https --config webpack.dev.js",
    "test": "jest",
    "prepare": "husky",
    "lint": "eslint src",
    "precommit": "npx lint-staged --diff=origin/main --verbose"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --fix",
    "**/*": "prettier --write --ignore-unknown"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:fntechgit/my-orders-tickets-widget.git"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "^14.5.2",
    "@types/react-dom": "16.9.24",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "browser-tabs-lock": "^1.2.15",
    "clean-webpack-plugin": "^4.0.0",
    "crypto-js": "^4.2.0",
    "css-loader": "^7.1.2",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "flush-promises": "^1.0.2",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "i18n-react": "^0.7.0",
    "identity-obj-proxy": "^3.0.0",
    "idtoken-verifier": "^2.2.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^15.1.0",
    "mini-css-extract-plugin": "^2.9.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "node-sass": "^9.0.0",
    "openstack-uicore-foundation": "^4.1.82",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "path": "^0.12.7",
    "prettier": "^2.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-mock-store": "^1.5.4",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "superagent": "8.0.9",
    "sweetalert2": "^11.12.0",
    "urijs": "^1.19.11",
    "url-loader": "^4.1.1",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^5.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "peerDependencies": {
    "crypto-js": "^4.2.0",
    "i18n-react": "^0.7.0",
    "idtoken-verifier": "^2.2.4",
    "openstack-uicore-foundation": "^4.1.82",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "sweetalert2": "^11.11.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "node_modules/react-rte"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "transform": {
      "\\.[jt]sx?$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ],
    "globals": {
      "window": {},
      "console": {}
    },
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/testSetupFile.js"
    ]
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.24.7",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "@reduxjs/toolkit": "^2.2.5",
    "history": "^5.3.0",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "react-i18next": "^14.1.2"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "react-app"
    ],
    "rules": {
      "no-unused-vars": "warn"
    },
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "max-len": "warn"
        }
      }
    ]
  }
}
